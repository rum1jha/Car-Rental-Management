  GRANT READ ON "ADMIN"."ANALYST_VIEW" TO "DEVELOPER";
  GRANT ON COMMIT REFRESH ON "ADMIN"."ANALYST_VIEW" TO "DEVELOPER";
  GRANT QUERY REWRITE ON "ADMIN"."ANALYST_VIEW" TO "DEVELOPER";
  GRANT DEBUG ON "ADMIN"."ANALYST_VIEW" TO "DEVELOPER";
